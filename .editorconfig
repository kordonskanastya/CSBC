root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline= lf
nsert_final_newline = true
# editorconfig-tools is unable to ignore logs strings or urls

max_line_length = 80

[{*.json,.eslintrc,.prettierrc}]
insert_final_newline = false

[*.md]
indent_size = false
